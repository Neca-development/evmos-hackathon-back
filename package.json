{
  "name": "unistory-nestjs-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "migration:create:development": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:create ./src/data/migrations/migration --outputJs",
    "migration:generate:development": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:generate ./src/data/migrations/migration -d ./src/data/datasource.ts --pretty --outputJs",
    "migration:up:development": "cross-env NODE_ENV=development ts-node ./node_modules/.bin/typeorm migration:run -d ./src/data/datasource.ts",
    "migration:down:development": "cross-env NODE_ENV=development ts-node ./node_modules/.bin/typeorm migration:revert -d ./src/data/datasource.ts",
    "migration:create:production": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:create ./src/data/migrations/migration --outputJs",
    "migration:generate:production": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:generate ./src/data/migrations/migration -d ./src/data/datasource.ts --pretty --outputJs",
    "migration:up:production": "cross-env NODE_ENV=production ts-node ./node_modules/.bin/typeorm migration:run -d ./src/data/datasource.ts",
    "migration:down:production": "cross-env NODE_ENV=production ts-node ./node_modules/.bin/typeorm migration:revert -d ./src/data/datasource.ts"
  },
  "dependencies": {
    "@automapper/classes": "^8.7.5",
    "@automapper/core": "^8.7.5",
    "@automapper/nestjs": "^8.7.5",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^9.0.0",
    "@unistory/nestjs-auth": "^1.0.4",
    "@unistory/nestjs-logger": "^1.0.6",
    "@unistory/route-decorators": "^1.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "ethers": "^5.7.0",
    "express": "^4.18.1",
    "express-http-context": "^1.2.4",
    "multer": "^1.4.5-lts.1",
    "nest-csv-parser": "^2.0.4",
    "nft.storage": "^7.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.7.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
